<% for domain in @domains %>
	<tr>
		<td class="nameColumn">
			<%= link_to h(domain.name), admin_domain_url(domain) %>
		</td>
		<td class="typeColumn">
			<%= h(domain.type) %>
		</td>
		<td class="ownerColumn">
			<%= h(domain.owner) %>
		</td>
		<td class="createdColumn">
			<%= h(domain.created_at.strftime('%Y-%m-%d %H:%M')) %>
		</td>
		<td class="changedColumn">
			<%= h(domain.updated_at.strftime('%Y-%m-%d %H:%M')) %>
		</td>
		<td class="actionsColumn">
			<%= link_to 'Details', admin_domain_url(domain) -%>,
			<%= link_to 'Delete', admin_domain_url(domain), :method => :delete %>
		</td>
	</tr>
<% end %>
